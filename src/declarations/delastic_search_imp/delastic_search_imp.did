type Result = 
 variant {
   err: vec Record;
   ok: vec Record;
 };
type RecordAttributes = 
 vec record {
       text;
       variant {"text": opt text;};
     };
type Record = 
 record {
   attributes: RecordAttributes;
   entityType: text;
   id: text;
 };
service : {
  queryIndex: (text, text) -> (Result) query;
  removeRecord: (text, vec text) -> () oneway;
  updateRecord: (Record, vec text, vec text) -> () oneway;
}
